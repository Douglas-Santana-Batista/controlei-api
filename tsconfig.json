{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "typeRoots": ["./node_modules/@types", "src/domain/types"],
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "outDir": "./dist",
    "rootDir": "./src",
    "baseUrl": "."
  },
  "include": ["src/**/*", "swagger.json"]
}
